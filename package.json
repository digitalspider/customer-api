{
  "name": "ds-customer-api",
  "version": "1.0.0",
  "description": "Digital Spider Customer APIs",
  "main": "index.js",
  "scripts": {
    "build": "sam build --beta-features",
    "deploy:dev": "sam deploy --profile customer --no-confirm-changeset --no-fail-on-empty-changeset --config-file samconfig-dev.toml",
    "deploy:staging": "sam deploy --profile customer --no-confirm-changeset --no-fail-on-empty-changeset --config-file samconfig-staging.toml",
    "deploy:prod": "sam deploy --profile customer --no-confirm-changeset --no-fail-on-empty-changeset --config-file samconfig-prod.toml",
    "clean": "node_modules/.bin/rimraf .aws-sam .vscode",
    "prebuild": "node_modules/.bin/rimraf .aws-sam .vscode",
    "prewatch": "node_modules/.bin/rimraf .aws-sam .vscode",
    "lint": "eslint . --ext .js,.ts --format=pretty",
    "lint:quiet": "yarn lint --quiet",
    "lint:fix": "yarn lint --quiet --fix",
    "validate": "yarn lint:quiet && yarn test",
    "test": "yarn jest",
    "start:api": "sam local start-api --log-file logs-api.log --env-vars env.json",
    "start": "yarn build --cached && yarn start:api"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@types/aws-lambda": "^8.10.137",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.10",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "esbuild": "^0.21.0",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "newman": "^5.3.2",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.569.0",
    "@aws-sdk/client-s3": "^3.569.0",
    "@aws-sdk/client-secrets-manager": "^3.569.0",
    "@aws-sdk/lib-dynamodb": "^3.569.0",
    "@aws-sdk/util-dynamodb": "^3.569.0",
    "aws-lambda": "^1.0.7",
    "axios": "^1.6.8",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalspider/lambda-function-url.git"
  },
  "author": "digitalspider",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/digitalspider/lambda-function-url/issues"
  },
  "homepage": "https://github.com/digitalspider/lambda-function-url#readme"
}
